  "bugs": {
    "url": "https://github.com/ocranbillions/johnnystore_api/issues"
  },
  "homepage": "https://github.com/ocranbillions/johnnystore_api#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "make-runnable": "^1.3.8",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/mysql": "^2.15.17",
    "@types/node": "^14.14.22",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3"
